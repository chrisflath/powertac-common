<plugin name='powertac-common' version='0.9' grailsVersion='1.3.6 &gt; *'>
  <author>Carsten Block</author>
  <title>Common domain classes and command objects for powertac competition</title>
  <description>\
This plugin provides common domain classes and command objects for powertac competition.
</description>
  <documentation>http://powertac.org/plugin/powertac-common</documentation>
  <resources>
    <resource>BuildConfig</resource>
    <resource>Config</resource>
    <resource>DataSource</resource>
    <resource>UrlMappings</resource>
    <resource>org.powertac.common.Broker</resource>
    <resource>org.powertac.common.CashUpdate</resource>
    <resource>org.powertac.common.Competition</resource>
    <resource>org.powertac.common.CustomerInfo</resource>
    <resource>org.powertac.common.HourlyCharge</resource>
    <resource>org.powertac.common.Orderbook</resource>
    <resource>org.powertac.common.PositionUpdate</resource>
    <resource>org.powertac.common.Product</resource>
    <resource>org.powertac.common.Rate</resource>
    <resource>org.powertac.common.Shout</resource>
    <resource>org.powertac.common.SimulationAction</resource>
    <resource>org.powertac.common.Tariff</resource>
