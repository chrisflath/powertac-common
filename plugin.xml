    <resource>org.powertac.common.command.ShoutDoUpdateCmd</resource>
    <resource>org.powertac.common.command.TariffDoRevokeCmd</resource>
    <resource>org.powertac.common.command.TariffDoSubscribeCmd</resource>
    <resource>org.powertac.common.ClockDriveJob</resource>
    <resource>org.powertac.common.TimeService</resource>
